<Window x:Class="QRiskTreeEditor.SecondaryWindows.AssociateMitigation"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:QRiskTreeEditor.SecondaryWindows"
        mc:Ignorable="d"
        Title="Associate a Mitigation" Height="175" Width="424" ResizeMode="NoResize">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Label x:Name="label" Content="Selected Item" Margin="10,10,0,0" Height="26" VerticalAlignment="Top" HorizontalAlignment="Left" Width="83"/>
        <TextBox x:Name="_parent" Margin="98,13,10,0" TextWrapping="Wrap" VerticalAlignment="Top" IsReadOnly="True" Height="22" Grid.ColumnSpan="2"/>
        <Label x:Name="label1" Content="Mitigation" Margin="10,42,0,0" Height="26" HorizontalAlignment="Left" VerticalAlignment="Top" Width="83" RenderTransformOrigin="0.512,0.682"/>
        <ComboBox x:Name="_items" Margin="98,48,10,0" d:LayoutOverrides="HorizontalAlignment" Height="22" VerticalAlignment="Top" Grid.ColumnSpan="2"/>
        <Button x:Name="_ok" Content="OK" Margin="130,100,209,10" Height="20" Width="65" Click="_ok_Click" VerticalAlignment="Top" HorizontalAlignment="Left"/>
        <Button x:Name="_cancel" Content="Cancel" Margin="229,100,110,10" Height="20" Width="65" Click="_cancel_Click" HorizontalAlignment="Left" VerticalAlignment="Top"/>
    </Grid>
</Window>
